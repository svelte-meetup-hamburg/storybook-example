{
  "name": "svelte-meetup-storybook",
  "version": "0.0.1",
  "type": "module",
  "svelte": "index.js",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "cypress open",
    "loki_update": "loki update",
    "loki_test": "loki test",
    "loki_approve": "loki approve"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@storybook/addon-actions": "6.4.0-beta.6",
    "@storybook/addon-essentials": "6.4.0-beta.6",
    "@storybook/addon-links": "6.4.0-beta.6",
    "@storybook/addon-svelte-csf": "1.1.0",
    "@storybook/svelte": "6.4.0-beta.6",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "autoprefixer": "10.3.7",
    "babel-loader": "8.2.2",
    "cssnano": "5.0.8",
    "cypress": "^8.6.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "loki": "0.28.1",
    "postcss": "8.3.9",
    "postcss-load-config": "3.1.0",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "2.4.0",
    "storybook-builder-vite": "0.1.0",
    "svelte": "3.43.1",
    "svelte-check": "2.2.7",
    "svelte-loader": "3.1.2",
    "svelte-preprocess": "4.9.8",
    "svelte2tsx": "0.4.7",
    "tailwindcss": "2.2.16",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1024,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  }
}